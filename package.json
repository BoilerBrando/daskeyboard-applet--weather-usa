{
  "name": "q-applet-weather",
  "displayName": "Q Weather Applet",
  "version": "1.0.0",
  "description": "Das Keyboard Q Applet for Weather Forecast",
  "longDescription": "Display the weather forecast on your Q keyboard.",
  "officialProduct": "",
  "appUrl": "",
  "isSingleton": false,
  "videoUrl": "",
  "icon": "images/icon.png",
  "image": "images/bigimage.png",
  "publisherName": "Das Keyboard",
  "authorName": "Das Keyboard team",
  "authorUrl": "https://twitter.com/daskeyboard",
  "issuesUrl": "https://twitter.com/daskeyboard",
  "developerRepoUrl": "https://github.com/metadot/q-applet-stock-quote",
  "licenseUrl": "",
  "changelogUrl": "",
  "qActionUrl": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha"
  },
  "author": "Cory Kim",
  "license": "MIT",
  "dependencies": {
    "daskeyboard-applet": "~2.4.3"
  },
  "qConfig": {
    "geometry": {
      "width": 4,
      "height": 1,
      "defaults": {
        "origin": {
          "x": 0,
          "y": 1
        }
      }
    },
    "applet": {
      "defaults": {}
    },
    "questions": [
      {
        "key": "zoneId",
        "label": "Choose your city",
        "help": "select the city from the dropdown",
        "required": true,
        "order": 1,
        "controlType": "dropdown",
        "dynamic": true,
        "options": []
      }
    ],
    "requirements": {
      "applet": {
        "zoneId": "select"
      }
    }
  },
  "devDependencies": {
    "mocha": "^5.2.0"
  }
