{
  "name": "q-applet-weather",
  "displayName": "Q Weather",
  "version": "1.0.0",
  "description": "Display weather forecast on a Das Keyboard Q",
  "longDescription": "Be aware of the upcoming storm, ice age or heat wave coming to your place with the Q Weather applet.",
  "officialProduct": "",
  "appUrl": "",
  "isSingleton": false,
  "videoUrl": "",
  "icon": "images/xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxicon.png",
  "image": "images/xxxxxxxxxxxxxxxxxxxxxxxxxxxxxbigimage.png",
  "publisherName": "Das Keyboard",
  "authorName": "Das Keyboard team",
  "authorUrl": "https://twitter.com/daskeyboard",
  "issuesUrl": "https://github.com/daskeyboard/q-applet-weather/issues",
  "homepage": "https://github.com/DasKeyboard/q-applet-weather",
  "developerRepoUrl": "https://github.com/daskeyboard/q-applet-stock-quote",
  "licenseUrl": "http://www.gnu.org/licenses/old-licenses/gpl-2.0.txt",
  "changelogUrl": "",
  "qActionUrl": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "daskeyboard-applet": "~2.4.3"
  },
  "qConfig": {
    "geometry": {
      "width": 4,
      "height": 1,
      "defaults": {
        "origin": {
          "x": 0,
          "y": 1
        }
      }
    },
    "applet": {
      "defaults": {}
    },
    "questions": [
      {
        "key": "zoneId",
        "label": "Choose a location",
        "help": "select a location from the list",
        "required": true,
        "order": 1,
        "controlType": "dropdown",
        "dynamic": true,
        "options": []
      }
    ],
    "requirements": {
      "applet": {
        "zoneId": "select"
      }
    }
  },
  "devDependencies": {
    "mocha": "^5.2.0"
  }
}
